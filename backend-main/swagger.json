  "components": {
    "securitySchemes": {
      "bearer": {
        "scheme": "bearer",
        "bearerFormat": "JWT",
        "type": "http"
      }
    },
    "schemas": {
      "CreateAuthPhoneDto": {
        "type": "object",
        "properties": {
          "phoneNumber": {
            "type": "string",
            "example": "0340408388"
          }
        },
        "required": [
          "phoneNumber"
        ]
      },
      "MessageResponseDto": {
        "type": "object",
        "properties": {
          "message": {
            "type": "string",
            "example": "Le code a été envoyé au numéro 034732..."
          }
        },
        "required": [
          "message"
        ]
      },
      "VerifingCodeDto": {
        "type": "object",
        "properties": {
          "phoneNumber": {
            "type": "string",
            "example": "0340408388"
          },
          "code": {
            "type": "string",
            "example": "1234"
          }
        },
        "required": [
          "phoneNumber",
          "code"
        ]
      },
      "VerifyUserDto": {
        "type": "object",
        "properties": {
          "_id": {
            "type": "string",
            "example": "65f123abc9d99e12aabb0001"
          },
          "name": {
            "type": "string",
            "example": "John Doe"
          },
          "compteFb": {
            "type": "string",
            "example": "john.doe"
          },
          "phoneNumber": {
            "type": "string",
            "example": "+261341234567"
          },
          "activated": {
            "type": "boolean",
            "example": true
          }
        },
        "required": [
          "_id",
          "name",
          "compteFb",
          "phoneNumber",
          "activated"
        ]
      },
      "VerifyCodeResponseDto": {
        "type": "object",
        "properties": {
          "message": {
            "type": "string",
            "example": "Code vérifié. Sélectionnez l'utilisateur à connecter."
          },
          "user": {
            "type": "array",
            "items": {
              "$ref": "#/components/schemas/VerifyUserDto"
            }
          },
          "statusCode": {
            "type": "number",
            "example": 200
          }
        },
        "required": [
          "message",
          "user",
          "statusCode"
        ]
      },
      "LoginChosenUserDto": {
        "type": "object",
        "properties": {
          "_id": {
            "type": "string",
            "example": "69130451810c61130917a1c6"
          },
          "phone": {
            "type": "string",
            "example": "0340408388"
          },
          "activated": {
            "type": "boolean",
            "example": true
          }
        },
        "required": [
          "_id",
          "phone",
          "activated"
        ]
      },
      "LoginResponseDto": {
        "type": "object",
        "properties": {
          "user": {
            "$ref": "#/components/schemas/VerifyUserDto"
          },
          "message": {
            "type": "string",
            "example": "Connexion réussie"
          },
          "statusCode": {
            "type": "number",
            "example": 200
          },
          "accessToken": {
            "type": "string",
            "example": "abcd.efgh.ijkl"
          },
          "refreshToken": {
            "type": "string",
            "example": "mnop.qrst.uvwx"
          }
        },
        "required": [
          "user",
          "message",
          "statusCode",
          "accessToken",
          "refreshToken"
        ]
      },
      "TokenDto": {
        "type": "object",
        "properties": {
          "token": {
            "type": "string",
            "example": "0bnm-347cvbn-327vbhnj-9bn50"
          }
        },
        "required": [
          "token"
        ]
      },
      "LogOutDto": {
        "type": "object",
        "properties": {
          "_id": {
            "type": "string",
            "example": "0bnm-347cvbn-327vbhnj-9bn50"
          }
        },
        "required": [
          "_id"
        ]
      },
      "CreateUserDto": {
        "type": "object",
        "properties": {
          "idUser": {
            "type": "string",
            "description": "Identifiant du customer venant du backend A",
            "example": "42"
          },
          "name": {
            "type": "string",
            "description": "Nom complet de l'utilisateur",
            "example": "Alice Doe"
          },
          "phoneNumber": {
            "type": "string",
            "description": "Numéro de téléphone de l'utilisateur",
            "example": "+261341234567"
          },
          "compteFb": {
            "type": "string",
            "description": "Compte Facebook de l'utilisateur",
            "example": "Miracle Quin"
          }
        },
        "required": [
          "idUser",
          "name"
        ]
      },
      "CreateUserResponseDto": {
        "type": "object",
        "properties": {
          "_id": {
            "type": "string",
            "example": "42"
          },
          "idUser": {
            "type": "string",
            "example": "42"
          },
          "name": {
            "type": "string",
            "example": "Alice Doe"
          },
          "phoneNumber": {
            "type": "string",
            "example": "+261341234567"
          },
          "compteFb": {
            "type": "string",
            "example": "Miracle Quin"
          },
          "_OtpCode": {
            "type": "string",
            "example": "1234"
          },
          "_OtpExpiresAt": {
            "type": "string",
            "example": "wsdfgthy.p875tef.r534t6yghb"
          },
          "activated": {
            "type": "boolean",
            "example": false
          },
          "reactivationDate": {
            "type": "string",
            "example": "534657ytuhgbvg"
          },
          "refreshToken": {
            "type": "string",
            "example": "sedr5657ytuhgnb gt756yuijymn nbv"
          }
        },
        "required": [
          "_id",
          "idUser",
          "name",
          "phoneNumber",
          "compteFb",
          "_OtpCode",
          "_OtpExpiresAt",
          "activated",
          "reactivationDate",
          "refreshToken"
        ]
      },
      "ResponseRegisterDto": {
        "type": "object",
        "properties": {
          "_id": {
            "type": "string",
            "example": "2"
          },
          "firstname": {
            "type": "string",
            "example": "Alice"
          },
          "lastname": {
            "type": "string",
            "example": "Doe"
          },
          "sex": {
            "type": "string",
            "example": "M",
            "enum": [
              "M",
              "F"
            ]
          },
          "phone": {
            "type": "string",
            "example": "+261341234567"
          },
          "adress": {
            "type": "string",
            "example": "Fianarantsoa"
          },
          "email": {
            "type": "string",
            "example": "jr@rj.j"
          },
          "birthdate": {
            "type": "string",
            "example": "1997-06-12T21:00:00.000+00:00"
          },
          "birthplace": {
            "type": "string",
            "example": "Antananarivo"
          },
          "compteFb": {
            "type": "string",
            "example": "https://facebook.com/jennie.kim"
          },
          "inactive": {
            "type": "boolean",
            "example": false
          }
        },
        "required": [
          "_id",
          "firstname",
          "lastname",
          "sex",
          "phone",
          "adress",
          "email",
          "birthdate",
          "birthplace",
          "compteFb",
          "inactive"
        ]
      },
      "ObjectId": {
        "type": "object",
        "properties": {}
      },
      "UpdateUserDto": {
        "type": "object",
        "properties": {
          "_id": {
            "example": "sdcfvghbnj",
            "allOf": [
              {
                "$ref": "#/components/schemas/ObjectId"
              }
            ]
          },
          "idUser": {
            "type": "string",
            "example": "12"
          }
        },
        "required": [
          "_id",
          "idUser"
        ]
      },
      "UserResponseDto": {
        "type": "object",
        "properties": {
          "_id": {
            "type": "string",
            "example": "65f123abc9d99e12aabb0001"
          },
          "idUser": {
            "type": "string",
            "example": "42"
          },
          "name": {
            "type": "string",
            "example": "Alice Doe"
          },
          "phoneNumber": {
            "type": "string",
            "example": "+261341234567"
          },
          "activated": {
            "type": "boolean",
            "example": true
          },
          "reactivationDate": {
            "type": "string",
            "example": "2025-02-01T10:00:00.000Z"
          },
          "refreshToken": {
            "type": "string",
            "example": "abcd.efgh.1234"
          },
          "email": {
            "type": "string",
            "example": "alice@example.com"
          }
        },
        "required": [
          "_id",
          "idUser",
          "name",
          "phoneNumber",
          "activated"
        ]
      },
      "UpdateUserResponseDto": {
        "type": "object",
        "properties": {
          "data": {
            "description": "Updated user details",
            "allOf": [
              {
                "$ref": "#/components/schemas/UserResponseDto"
              }
            ]
          },
          "message": {
            "type": "string",
            "example": "Informations mises à jour avec succès"
          },
          "statusCode": {
            "type": "number",
            "example": 200
          }
        },
        "required": [
          "data",
          "message",
          "statusCode"
        ]
      },
      "CreateRegisterDto": {
        "type": "object",
        "properties": {
          "firstname": {
            "type": "string",
            "example": "Alice"
          },
          "lastname": {
            "type": "string",
            "example": "Doe"
          },
          "sex": {
            "type": "string",
            "example": "M",
            "enum": [
              "M",
              "F"
            ]
          },
          "phone": {
