    }
  ],
  "components": {
    "securitySchemes": {
      "bearer": {
        "scheme": "bearer",
        "bearerFormat": "JWT",
        "type": "http"
      }
    },
    "schemas": {
      "CreateAuthPhoneDto": {
        "type": "object",
        "properties": {
          "phoneNumber": {
            "type": "string",
            "example": "+261349482910"
          }
        },
        "required": [
          "phoneNumber"
        ]
      },
      "ObjectId": {
        "type": "object",
        "properties": {}
      },
      "CreateUserDto": {
        "type": "object",
        "properties": {
          "idUser": {
            "type": "string",
            "description": "Identifiant du customer venant du backend A",
            "example": "42"
          },
          "name": {
            "type": "string",
            "description": "Nom complet de l'utilisateur",
            "example": "Alice Doe"
          },
          "phoneNumber": {
            "type": "string",
            "description": "Numéro de téléphone de l'utilisateur",
            "example": "+261341234567"
          },
          "compteFb": {
            "type": "string",
            "description": "Compte Facebook de l'utilisateur",
            "example": "Miracle Quin"
          }
        },
        "required": [
          "idUser"
        ]
      },
      "UpdateUserDto": {
        "type": "object",
        "properties": {
          "_id": {
            "example": "sdcfvghbnj",
            "allOf": [
              {
                "$ref": "#/components/schemas/ObjectId"
              }
            ]
          },
          "idUser": {
            "type": "string",
            "example": "12"
          }
        },
        "required": [
          "_id",
          "idUser"
        ]
      },
      "CreateRegisterDto": {
        "type": "object",
        "properties": {
          "firstname": {
            "type": "string",
            "description": "First name",
            "example": "Alice"
          },
          "lastname": {
            "type": "string",
            "description": "Last name",
            "example": "Doe"
          },
          "sex": {
            "type": "string",
            "description": "Genre l'utilisateur",
            "example": "M"
          },
          "phone": {
            "type": "string",
            "description": "Numéro de téléphone de l'utilisateur",
            "example": "+261341234567"
          },
          "adress": {
            "type": "string",
            "description": "Adresse de l'utilisateur",
            "example": "Fianarantsoa"
          },
          "email": {
            "type": "string",
            "description": "Email de l'utilisateur",
            "example": "jr@rj.j"
          },
          "birthdate": {
            "format": "date-time",
            "type": "string",
            "description": "Date d'anniversaire de l'utilisateur",
            "example": "1997-06-12T21:00:00.000+00:00"
          },
          "birthplace": {
            "type": "string",
            "description": "Lieu de naissance de l'utilisateur",
            "example": "Antananarivo"
          },
          "compteFb": {
            "type": "string",
            "description": "Compte Facebook de l'utilisateur",
            "example": "https://facebook.com/jennie.kim"
          },
          "inactive": {
            "type": "boolean",
            "description": "Compte actif",
            "example": "false"
          }
        },
        "required": [
          "firstname",
          "phone",
          "birthdate",
          "birthplace",
          "inactive"
        ]
      }
    }
  }
